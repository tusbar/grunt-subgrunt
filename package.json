{
  "name": "grunt-subgrunt",
  "version": "1.3.0",
  "description": "Run sub-projects' grunt tasks.",
  "repository": {
    "type": "git",
    "url": "git://github.com/tusbar/grunt-subgrunt.git"
  },
  "scripts": {
    "test": "grunt test",
    "lint": "xo && (cd test/fixtures && xo)"
  },
  "keywords": [
    "gruntplugin",
    "subgrunt",
    "subproject",
    "submodule"
  ],
  "author": {
    "name": "Bertrand Marron",
    "url": "https://github.com/tusbar"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tusbar/grunt-subgrunt/issues"
  },
  "dependencies": {
    "async": "^3.2.0",
    "glob": "^7.0.0"
  },
  "peerDependencies": {
    "grunt": ">=0.4.0"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-nodeunit": "^2.0.0",
    "xo": "^0.38.2"
  },
  "engines": {
    "node": ">=6"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "unicorn/filename-case": "off"
    }
  }
}
